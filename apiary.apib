                                  "notification": "5245bc5b3e66a3b01f0024d4",
                                  "rules": [],
                                  "skipDays": [],
                                  "source": "5245bc5b3e66a3b01f0024d4",
                                  "active": false
                            },
                            {
                                  "name": "On Store exit",
